{
  "name": "FakeNewsDetector",
  "description": "Check if a text of a news is Fake or Fact",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo-fnd.png"
  },
  "icons": {
   "16": "logo-fnd.png",
   "32": "logo-fnd.png",
   "48": "logo-fnd.png",
   "128": "logo-fnd.png"
    },
  "host_permissions": ["http://localhost:5000/"]
}